<head>
    <link rel="stylesheet" href="/public/stylesheets/style-libro.css">
</head>
<main>
    <div class="aviso">
        <img class="alerta" src="data:image/svg+xml;base64," />
        <p>El carrito y el buscador aun no funcionan. Tampoco el boton de "agregar a carrito".</p>
        <img class="alerta" src="data:image/svg+xml;base64," />
    </div>
    <h1><%= libros.titulo %></h1>
    <div class="presentacion">
        <img class="tapas" src="<%= libros.tapa %>" alt="">
        <div class="informacion">
            <p class="precio">$<%= libros.precio %></p>
            <form class="afirmacion" action="/carrito" method="POST">
                <p class="stock">Stock: <%= libros.stock %></p>
                <button class="boton" type="submit">Agregar carrito</button>
            </form>
        </div>
    </div>
    <div class="datos">
        <p id="descripcion"><%= libros.descripcion %> </p>
        <p>Autor: <%= libros.autor %> </p>
        <p>Genero: <%= libros.genero %> </p>
        <p>Tipo: <%= libros.tipo %> </p>
        <p>Publico: <%= libros.publico %> </p>
        <p>Tamaño: <%= libros.tamaño %>cm </p>
    </div>
    <div class="atajos">
        <ul class="atajos-lista">
            <li class="atajos-iconos">
                <a href="/catalogo/" class="catalogo">
                    <img src="data:image/svg+xml;base64,"/>
                    <p>catalogo</p>
                </a>
            </li>
        </ul>
    </div>
</main>
<script>
    let botoncito = document.getElementsByClassName('boton')[0];
    let changoItem = document.getElementsByClassName('cantidad')[0];
    let resultado = 0;
    botoncito.onclick = () => {
        let changos = changoItem.innerText;
        resultado = resultado + 1;
        changos = resultado;
        changoItem.innerHTML = changos;
    }
</script>
